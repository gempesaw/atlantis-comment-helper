{
  "name": "atlantis comment helper",
  "version": "1.0.0",
  "description": "make it easier to input a previous atlantis command multiple times",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/pull/*",
        "https://github.com/*/issues/*"
      ],
      "js": ["atlantis-comment-helper.js"],
      "css": ["atlantis-comment-helper.css"]
    }
  ]
}
